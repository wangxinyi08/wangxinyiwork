
type student struct {
	name  string
	score int
}

func findmax(a []student, n int) int {
	s := n
	for i := n + 1; i < len(a); i++ {
		if a[s].score < a[i].score {
			s = i
		}
	}
	return s
}

func sort(a []student) {
	for i := 0; i < len(a); i++ {
		s := findmax(a, i)
		a[s], a[i] = a[i], a[s]
	}
}

a := [student{"a", 95}, student{"b", 92}, student{"c", 90}]

sort(a)
println(a)
